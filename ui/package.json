{
  "name": "ui",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "up": "taze major -I",
    "gen:env": "npx import-meta-env-typescript -x .env.example -o ./src ",
    "swagger": "node scripts/fetchAndWrite.cjs && pnpm dlx swagger-typescript-api -p ./swagger.json -o ./src/services/api --responses --union-enums --route-types  --extract-request-params --extract-request-body --extract-response-body   --unwrap-response-data --sort-types --modular  --extract-enums",
    "postinstall": "pnpm gen:env && pnpm run swagger",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@anu-vue/preset-theme-default": "^0.15.2",
    "@lighthouse-web3/sdk": "^0.3.0",
    "@pushprotocol/restapi": "^1.4.45",
    "@safe-global/auth-kit": "^2.0.0",
    "@unhead/vue": "^1.8.8",
    "@unocss/reset": "^0.58.0",
    "@vueuse/core": "^10.6.1",
    "@vueuse/head": "^2.0.0",
    "@waku/sdk": "^0.0.21",
    "@web3auth/safeauth-embed": "^0.0.0",
    "anon-aadhaar-pcd": "^0.1.14",
    "anu-vue": "^0.15.2",
    "nprogress": "^0.2.0",
    "ohash": "^1.1.3",
    "pinia": "^2.1.7",
    "protobufjs": "^7.2.5",
    "use-wagmi": "0.4.6",
    "viem": "^1.19.11",
    "vue": "^3.3.10",
    "vue-demi": "^0.14.6",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.3.1",
    "@iconify-json/bx": "^1.1.9",
    "@iconify-json/carbon": "^1.1.24",
    "@iconify-json/mdi": "^1.1.55",
    "@import-meta-env/typescript": "^0.3.2",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.58.0",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vue-macros/volar": "^0.17.4",
    "@wagmi/cli": "^1.5.2",
    "critters": "^0.0.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-format": "^0.0.1",
    "https-localhost": "^4.7.1",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.11.0",
    "rollup": "^4.6.1",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.0",
    "typescript": "^5.3.2",
    "unocss": "^0.58.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-turbo-console": "1.0.6-beta.1",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-macros": "^2.7.0",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.0.5",
    "vite-bundle-visualizer": "^0.11.0",
    "vite-plugin-inspect": "^0.8.1",
    "vite-plugin-node-polyfills": "^0.17.0",
    "vite-plugin-pwa": "^0.17.2",
    "vite-plugin-vue-devtools": "^1.0.0-rc.6",
