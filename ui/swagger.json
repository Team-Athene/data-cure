  "info": {
    "title": "Data Cure api",
    "description": "main api",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "S3Dto": {
        "type": "object",
        "properties": {
          "bucket": {
            "type": "string",
            "example": "datacure-dapp"
          },
          "key": {
            "type": "string",
            "example": "8Qyo70ZCgaoOozu4Zgnit79NUiEaZsYj8iC7GNS9"
          },
          "region": {
            "type": "string",
            "example": "ap-south-1"
          },
          "accessKeyId": {
            "type": "string",
            "example": "AKIAZB27L43CC3JJNZX2"
          }
        },
        "required": [
          "bucket",
          "key",
          "region",
          "accessKeyId"
        ]
      },
      "UserDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Dennis Sam"
          },
          "aadhar": {
            "type": "number",
            "example": "123443218765"
          },
          "age": {
            "type": "string",
            "example": "31-45"
          },
          "gender": {
            "type": "string",
            "example": "Male"
          },
          "bloodGroup": {
            "type": "string",
            "example": "B+"
          },
          "ethnicity": {
            "type": "string",
            "example": "Caucasian"
          },
          "sale": {
            "type": "boolean",
            "example": "true"
          }
        },
        "required": [
          "name",
          "aadhar",
          "age",
          "gender",
          "bloodGroup",
          "ethnicity",
          "sale"
        ]
      },
      "UploadDto": {
        "type": "object",
        "properties": {
          "s3": {
            "$ref": "#/components/schemas/S3Dto"
          },
          "user": {
            "$ref": "#/components/schemas/UserDto"
          }
        },
        "required": [
          "s3",
          "user"
        ]
      },
      "IPNSDto": {
        "type": "object",
        "properties": {
          "userCid": {
            "type": "string",
            "example": "QmYm1QZ1Dq6D2Kj7Z4BQ9wvJZgZ8J3YQW5Y4U1S1L2D2Kj7Z4BQ9wvJZgZ8J3YQW5Y4U1S1L"
          },
          "ipnsName": {
            "type": "string",
            "example": "1234abcd"
          }
        },
        "required": [
          "userCid",
          "ipnsName"
        ]
      }
    }
  }
}